 <RecipeDef ParentName="SurgeryXTails">
      <defName>InstallGnollTail</defName>
      <label>install gnoll tail</label>
      <description>Replace missing or damaged tail.</description>
      <workerClass>Recipe_InstallNaturalBodyPart</workerClass>
      <jobString>Installing tail.</jobString>
      <workAmount>1200</workAmount>
      <ingredients>
         <li>
            <filter>
               <categories>
                  <li>Medicine</li>
               </categories>
            </filter>
            <count>1</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>GnollTail</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
         <categories>
            <li>Medicine</li>
         </categories>
         <thingDefs>
            <li>GnollTail</li>
         </thingDefs>
      </fixedIngredientFilter>
      <appliedOnFixedBodyParts>
         <li>GnollTail</li>
      </appliedOnFixedBodyParts>
   </RecipeDef>

